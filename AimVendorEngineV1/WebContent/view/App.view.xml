<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" xmlns:u="sap.ui.unified" controllerName="sap.ui.medApp.view.App"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<u:Shell id="myShell" icon="assets/img/logo1.png">
		<u:headItems>
			<u:ShellHeadItem tooltip="Home" icon="sap-icon://home"
				press="handelHomeBtn" visible="false" />
		</u:headItems>
		<u:user>
			<u:ShellHeadUserItem visible="false"
				image="sap-icon://person-placeholder" username="{/LoggedUser/USRNM}"
				press="settingsSelect" />
		</u:user>
		<u:content>
			<Shell id="mShell"></Shell>
		</u:content>
	</u:Shell>
</core:View>