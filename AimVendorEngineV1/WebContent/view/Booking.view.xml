<mvc:View controllerName="sap.ui.medApp.view.Booking"
	xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" xmlns:f="sap.ui.layout.form"
	xmlns:c="sap.ui.core" xmlns="sap.m">
	<Page id="booking" title="{i18n>BOOKING_TITLE}">
		<content>
			<Table id="idBookings" inset="true" class="rightBlackBorder"
				items="{
			path: '/vendorRules'
			 
		}">

				<columns>
					<Column width="12em">
						<Text text="{i18n>Time}" />
					</Column>
					<Column minScreenWidth="Tablet" demandPopin="true">
						<Text text="{i18n>APPOINTMENTS}" />
					</Column>

				</columns>

				<headerToolbar>
					<Toolbar>
						<Title text="Appointment Date:" level="H2" />
						<Title id="dateTitle" level="H2" />
						<ToolbarSpacer></ToolbarSpacer>
						<Select change="handleEntityChange" id="entitySelect"
							items="{
            path: '/vendorsList/0/Rules',
            sorter: { path: 'ENTID' }
          }">
							<c:Item key="{ENTID}" text="{DESCR}" />
						</Select>

					</Toolbar>
				</headerToolbar>
				<items>
					<ColumnListItem>
						<cells>

							<Text text="{START}" />
							<FlexBox alignItems="Start" items="{
									path: 'BOOKINGS'}">
								<items>
									<Button text="{TITLE}   {FRNAM}  {LTNAM}" />
								</items>
							</FlexBox>


						</cells>
					</ColumnListItem>
				</items>
			</Table>
		</content>
	</Page>
</mvc:View>